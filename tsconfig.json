{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": ["src", "custom.d.ts"],
  "files": ["custom.d.ts"]
}
